<h2>配送先編集</h2>

<%= form_with model: @address, url: address_path(@address) do |f| %>
  <%= f.label :郵便番号 %>
  <%= f.text_field :postal_code %><br>
  
  <%= f.label :住所 %>
  <%= f.text_field :address %><br>
  
  <%= f.label :宛名 %>
  <%= f.text_field :name %><br>
  
  <%= f.submit "変更内容を保存", class:"btn btn-success" %>
<% end %>